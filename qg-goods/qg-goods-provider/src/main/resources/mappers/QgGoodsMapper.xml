<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qg.mapper.QgGoodsMapper">


    <update id="qdtxModifyQgGoods">
        UPDATE
        qg_goods
        SET
        stock= stock-#{stock}
        WHERE id = #{id}
    </update>
</mapper>